    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5",
    "webpack-stats-plugin": "^0.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
